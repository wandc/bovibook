        $pattern = '/XNC_COMP_PRECIP_RAIN_.*\.GIF/';
        $dude = preg_match_all($pattern, $newStr, $matches, PREG_OFFSET_CAPTURE);


//var_dump($matches);
//create full URL array for each radar gif
